{% extends 'base.html.twig' %}

{% block title %}Company{% endblock %}

{% block body %}
    <h1>Mi empresa</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Nombre</th>
                <td>{{ company.title }}</td>
            </tr>
            <tr>
                <th>Nif</th>
                <td>{{ company.nif }}</td>
            </tr>
            <tr>
                <th></th>
                <td>{{ company.ccc }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{path("app_register", {'nif': company.nif})}}">Registrar un nuevo trabajador en la empresa</a>
{% endblock %}

